#!/bin/bash
if [[ -z $DEVROOT ]]; then
	  echo "export DEVROOT=/path/to/projects; then continue"
	  exit 0;
fi

HIVE=hive-0.12.0-transwarp
NGMR=ngmr-1.7-transwarp

echo "Copying for Jenkins regression tests ..."
rm -rf ~/jars/*

rsync -aXS $DEVROOT/$HIVE/src/metastore/target/hive-metastore-0.12.0-transwarp-tdh460.jar $DEVROOT/$HIVE/src/hbase-handler/target/hive-hbase-handler-0.12.0-transwarp-tdh460.jar $DEVROOT/$HIVE/src/service/target/hive-service-0.12.0-transwarp-tdh460.jar $DEVROOT/$HIVE/src/cli/target/hive-cli-0.12.0-transwarp-tdh460.jar $DEVROOT/$HIVE/src/serde/target/hive-serde-0.12.0-transwarp-tdh460.jar $DEVROOT/$HIVE/src/hwi/target/hive-hwi-0.12.0-transwarp-tdh460.jar $DEVROOT/$HIVE/src/common/target/hive-common-0.12.0-transwarp-tdh460.jar $DEVROOT/$HIVE/src/ql/target/hive-exec-0.12.0-transwarp-tdh460.jar $DEVROOT/$HIVE/src/beeline/target/hive-beeline-0.12.0-transwarp-tdh460.jar $DEVROOT/$HIVE/src/contrib/target/hive-contrib-0.12.0-transwarp-tdh460.jar $DEVROOT/$HIVE/src/jdbc/target/hive-jdbc-0.12.0-transwarp-tdh460.jar  ~/jars
rsync -aXS $DEVROOT/$HIVE/src/shims/target/hive-shims-aggregator-0.12.0-transwarp-tdh460-combined.jar ~/jars/hive-shims-0.12.0-transwarp-tdh460.jar

rsync -aXS $DEVROOT/$NGMR/idbc/jdbc/target/scala-2.10/jdbcdrive-1.0.0-transwarp-tdh460.jar ~/jars
rsync -aXS $DEVROOT/$NGMR/idbc/hyperdrive/target/scala-2.10/hyperdrive-1.0.0-transwarp-tdh460.jar ~/jars
rsync -aXS $DEVROOT/$NGMR/idbc/core/target/scala-2.10/idbc-core-1.0.0-transwarp-tdh460.jar ~/jars
rsync -aXS $DEVROOT/$NGMR/idbc/esdrive/target/scala-2.10/esdrive-1.0.0-transwarp-tdh460.jar  ~/jars

rsync -aXS $DEVROOT/$NGMR/spark/stargate/target/scala-2.10/spark-stargate_2.10-1.1.0-transwarp-tdh460.jar ~/jars/ngmr-stargate_2.10-1.1.0-transwarp-tdh460.jar
rsync -aXS $DEVROOT/$NGMR/spark/core/target/scala-2.10/spark-core_2.10-1.1.0-transwarp-tdh460.jar  ~/jars/ngmr-core_2.10-1.1.0-transwarp-tdh460.jar
rsync -aXS $DEVROOT/$NGMR/spark/holodesk/target/scala-2.10/spark-holodesk_2.10-1.1.0-transwarp-tdh460.jar ~/jars/ngmr-holodesk_2.10-1.1.0-transwarp-tdh460.jar
rsync -aXS $DEVROOT/$NGMR/inceptor/target/scala-2.10/inceptor_2.10-1.1.0-transwarp-tdh460.jar ~/jars/ngmr-shell_2.10-1.1.0-transwarp-tdh460.jar

echo "Copying for dell regression tests ..."
mkdir -p ~/jars-dell/hive
mkdir -p ~/jars-dell/ngmr
rm -rf ~/jars-dell/hive/*.jar
rm -rf ~/jars-dell/ngmr/*.jar

rsync -aXS  ~/.m2/repository/org/apache/hive/hive-beeline/0.12.0-transwarp-tdh460/hive-beeline-0.12.0-transwarp-tdh460.jar                                     ~/jars-dell/hive 
rsync -aXS  ~/.m2/repository/org/apache/hive/hive-cli/0.12.0-transwarp-tdh460/hive-cli-0.12.0-transwarp-tdh460.jar                                             ~/jars-dell/hive
rsync -aXS  ~/.m2/repository/org/apache/hive/hive-common/0.12.0-transwarp-tdh460/hive-common-0.12.0-transwarp-tdh460.jar                                       ~/jars-dell/hive
rsync -aXS  ~/.m2/repository/org/apache/hive/hive-contrib/0.12.0-transwarp-tdh460/hive-contrib-0.12.0-transwarp-tdh460.jar                                     ~/jars-dell/hive
rsync -aXS  ~/.m2/repository/org/apache/hive/hive-exec/0.12.0-transwarp-tdh460/hive-exec-0.12.0-transwarp-tdh460.jar                                           ~/jars-dell/hive
rsync -aXS  ~/.m2/repository/org/apache/hive/hive-hbase-handler/0.12.0-transwarp-tdh460/hive-hbase-handler-0.12.0-transwarp-tdh460.jar                         ~/jars-dell/hive
rsync -aXS  ~/.m2/repository/org/apache/hive/hive-hwi/0.12.0-transwarp-tdh460/hive-hwi-0.12.0-transwarp-tdh460.jar                                             ~/jars-dell/hive
rsync -aXS  ~/.m2/repository/org/apache/hive/hive-jdbc/0.12.0-transwarp-tdh460/hive-jdbc-0.12.0-transwarp-tdh460.jar                                           ~/jars-dell/hive
rsync -aXS  ~/.m2/repository/org/apache/hive/hive-metastore/0.12.0-transwarp-tdh460/hive-metastore-0.12.0-transwarp-tdh460.jar                                 ~/jars-dell/hive
rsync -aXS  ~/.m2/repository/org/apache/hive/hive-serde/0.12.0-transwarp-tdh460/hive-serde-0.12.0-transwarp-tdh460.jar                                         ~/jars-dell/hive
rsync -aXS  ~/.m2/repository/org/apache/hive/hive-service/0.12.0-transwarp-tdh460/hive-service-0.12.0-transwarp-tdh460.jar                                     ~/jars-dell/hive
rsync -aXS  ~/.m2/repository/org/apache/hive/hive-shims-aggregator/0.12.0-transwarp-tdh460/hive-shims-aggregator-0.12.0-transwarp-tdh460-combined.jar          ~/jars-dell/hive/hive-shims-0.12.0-transwarp-tdh460.jar

rsync -aXS  $DEVROOT/$NGMR/spark/core/target/scala-2.10/spark-core_2.10-1.1.0-transwarp-tdh460.jar             ~/jars-dell/ngmr/ngmr-core_2.10-1.1.0-transwarp-tdh460.jar 
rsync -aXS  $DEVROOT/$NGMR/spark/holodesk/target/scala-2.10/spark-holodesk_2.10-1.1.0-transwarp-tdh460.jar     ~/jars-dell/ngmr/ngmr-holodesk_2.10-1.1.0-transwarp-tdh460.jar
rsync -aXS  $DEVROOT/$NGMR/spark/stargate/target/scala-2.10/spark-stargate_2.10-1.1.0-transwarp-tdh460.jar     ~/jars-dell/ngmr/ngmr-stargate_2.10-1.1.0-transwarp-tdh460.jar

rsync -aXS  $DEVROOT/$NGMR/inceptor/target/scala-2.10/inceptor_2.10-1.1.0-transwarp-tdh460.jar                 ~/jars-dell/ngmr/ngmr-shell_2.10-1.1.0-transwarp-tdh460.jar
rsync -aXS  $DEVROOT/$NGMR/idbc/core/target/scala-2.10/idbc-core-1.0.0-transwarp-tdh460.jar                    ~/jars-dell/ngmr/idbc-core-1.0.0-transwarp-tdh460.jar
rsync -aXS  $DEVROOT/$NGMR/idbc/hyperdrive/target/scala-2.10/hyperdrive-1.0.0-transwarp-tdh460.jar             ~/jars-dell/ngmr/hyperdrive-1.0.0-transwarp-tdh460.jar
rsync -aXS  $DEVROOT/$NGMR/idbc/jdbc/target/scala-2.10/jdbcdrive-1.0.0-transwarp-tdh460.jar                    ~/jars-dell/ngmr/jdbcdrive-1.0.0-transwarp-tdh460.jar
rsync -aXS  $DEVROOT/$NGMR/idbc/esdrive/target/scala-2.10/esdrive-1.0.0-transwarp-tdh460.jar                   ~/jars-dell/ngmr/esdrive-1.0.0-transwarp-tdh460.jar
